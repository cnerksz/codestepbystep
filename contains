def contains(a1,a2):
    c=0
    for i in range(0,len(a1)):
        for j in range(0,len(a2)):
            if i<=len(a1)-3 and j<=len(a2)-3:
                if a1[i]==a2[j] and a1[i+2]==a2[j+2] and a1[i+1]==a2[j+1] :
                    c+=1
                else:
                    continue
    if c>=1:
        return True
    else:
        return  False
    


